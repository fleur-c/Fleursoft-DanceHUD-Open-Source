#ifndef UTILOWNERSAY
#define UTILOWNERSAY

OwnerSay(string msgToSend, list paramsForOwnerSay)
{
    llMessageLinked(LINK_THIS, Define_LANGUAGELINKID, msgToSend, llDumpList2String(paramsForOwnerSay, "|"));
}
#endif
